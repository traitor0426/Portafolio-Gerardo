<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowClearUserData="true"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GamerOver"
        tools:targetApi="31">
        <activity
            android:name=".InfoCompra"
            android:exported="false"
            android:label="@string/title_activity_info_compra"
            android:theme="@style/Theme.GamerOver" />
        <activity
            android:name=".Pruebas"
            android:exported="false"
            android:label="@string/title_activity_pruebas"
            android:theme="@style/Theme.GamerOver" />
        <activity
            android:name=".RegistroCompra"
            android:exported="false"
            android:label="@string/title_activity_registro_compra"
            android:theme="@style/Theme.GamerOver" />
        <activity
            android:name=".InfoVideojuegos"
            android:exported="false"
            android:label="@string/title_activity_info_videojuegos"
            android:theme="@style/Theme.GamerOver" />
        <activity
            android:name=".Inicio"
            android:exported="false"
            android:label="@string/title_activity_inicio"
            android:theme="@style/Theme.GamerOver" />
        <activity
            android:name=".VentanaRegistrarCuenta"
            android:exported="false"
            android:label="@string/title_activity_ventana_registrar_cuenta"
            android:theme="@style/Theme.GamerOver" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.GamerOver">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>